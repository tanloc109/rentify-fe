    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.28.2",
    "react-toastify": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
